}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.UserBox {\r\n  width: 34px;\r\n  height: 34px;\r\n  /* padding: 8px 0; */\r\n  /* margin-right: 100px ; */\r\n\r\n  .img {\r\n    width: 34px;\r\n    height: 34px;\r\n    background: none center center/cover;\r\n    border-radius: 50%;\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"]}]}