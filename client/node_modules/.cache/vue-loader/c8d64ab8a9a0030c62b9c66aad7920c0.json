Desktop/Resource/Webstudy/13-vue3/二次学习/vue08/打包上线版本/v1.1/client/src/views/ArticleID.vue","sourceRoot":"","sourcesContent":["\n<template>\n  <div id=\"ArticleID\">\n    <article>\n      <main id=\"mdToHTML\"></main>\n    </article>\n  </div>\n</template>\n\n<script>\nexport default {\n  async mounted() {\n    let id = this.$route.params.id;\n    let { data } = await this.$axios({\n      method: \"GET\",\n      url: \"/get/articleID\",\n      params: {\n        id,\n      },\n    });\n    if (data.code) {\n      return this.$message({\n        type: \"error\",\n        message: data.msg,\n        duration: 1000,\n        onClose: () => {\n          this.$router.replace(\"/article\");\n        },\n      });\n    }\n    let res = await this.$axios({\n      method: \"GET\",\n      url: `${data.data.md}`,\n    });\n    console.log(res);\n    editormd.markdownToHTML(\"mdToHTML\", {\n      markdown: res.data,\n      emoji: true,\n    });\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n#ArticleID {\n  #mdToHTML {\n    box-sizing: border-box;\n  }\n}\n</style>"]}]}