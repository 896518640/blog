class=\"code\">\r\n          {{userInfo.user}}\r\n        </span> \r\n    </el-form-item>\r\n    <el-form-item label=\"Êñ∞Áî®Êà∑ÂêçÔºö\" prop=\"user\">\r\n      <el-input v-model=\"userForm.user\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item>\r\n      <el-button type=\"primary\"  @click=\"submit('userForm')\" plain>Á°ÆËÆ§‰øÆÊîπ</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapState,mapMutations } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      userForm: {\r\n        user: \"\",\r\n      },\r\n      rules:{\r\n           user:[\r\n              {\r\n                  validator:(rule,value,cb)=>{\r\n                      //ÈïøÂ∫¶Ê£ÄÈ™å\r\n                      if(value.length<2 || value.length>8){\r\n                          cb(new Error(\"ËØ∑ËæìÂÖ•2~8‰ΩçÁöÑÁî®Êà∑Âêç\"))\r\n                      }\r\n                      //ËßÑÂàôÈ™åËØÅ\r\n                      if(!/^[\\w\\u4e00-\\u9fa5\\u0800-\\u4e00\\uac00-\\ud7ff]+$/.test(value)){\r\n                          cb(new Error(\"Âè™ÂÖÅËÆ∏Â≠óÊØç„ÄÅÊï∞Â≠ó„ÄÅ_„ÄÅ‰∏≠/Êó•/Èü©Êñá\"))\r\n                      }\r\n                      //‰∏çËÉΩÂíåÂéüÊù•ÁöÑ‰∏ÄÊ†∑\r\n                      if(value === this.userInfo.user){\r\n                          cb(new Error(\"Áî®Êà∑ÂêçÊ≤°ÊúâÂèòÂåñüòí\"))\r\n                      }\r\n                      cb()\r\n                  }\r\n              }\r\n          ],\r\n      }\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState([\"userInfo\"]),\r\n  },\r\n  methods: {\r\n      ...mapMutations(['loginSuccess']),\r\n      submit(){\r\n          this.$refs['userForm'].validate(async v=>{\r\n              if(v){\r\n                  let {data} = await this.$axios({\r\n                      method:\"POST\",\r\n                      url:\"/personal/user\",\r\n                      data:{\r\n                          user:this.userForm.user\r\n                      }\r\n                  })\r\n                  //‰øÆÊîπÂ§±Ë¥•\r\n                  if(data.code){\r\n                      return this.$message.error(data.msg)\r\n                  }\r\n                  //‰øÆÊîπÊàêÂäü\r\n                  this.$message.success(\"Áî®Êà∑Âêç‰øÆÊîπÊàêÂäü\")//ÊèêÁ§∫\r\n                  this.loginSuccess(data.data)//vuexÊõ¥Êñ∞\r\n                  this.$refs['userForm'].resetFields()//Ë°®ÂçïÈáçÁΩÆ\r\n              }else{\r\n                  return false\r\n              }\r\n          })\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.UpdateName{\r\n    font-family: Quicksand;\r\n  span.code{\r\n      color: #bd4147;\r\n      background-color: rgba(27,31,35,.05);\r\n      font-weight: 700;\r\n  }\r\n}\r\n\r\n</style>"]}]}