le.vue"],"names":[],"mappings":";;;;;;;;AAEO,EAAA,EAAE,EAAC;;;AAGC,EAAA,KAAK,EAAC;;;AACN,EAAA,KAAK,EAAC;;;AACN,EAAA,KAAK,EAAC;;;AAKN,EAAA,KAAK,EAAC;;;AAKJ,EAAA,KAAK,EAAC;;;AAER,EAAA,KAAK,EAAC;;;+CAEiC,M;;;;;;;wCAnBhD,mBAAA,CAsBM,KAtBN,EAAA,UAAA,EAsBM,E,kBArBJ,mBAAA,CAoBU,SApBV,EAoBU,IApBV,EAoBU,WAAA,CApB6B,KAAA,CAAA,WAoB7B,EApBwC,UAAnB,IAAmB,EAAf;yBAAnC,mBAAA,CAoBU,SApBV,EAoBU;AApBD,MAAA,KAAK,EAAC,QAoBL;AApB2C,MAAA,GAAG,EAAE,IAAI,CAAC;AAoBrD,KApBV,E,CACE,mBAAA,CAAyB,IAAzB,EAAyB,IAAzB,EAAyB,gBAAA,CAAlB,IAAI,CAAC,KAAa,CAAzB,EAAiB;AAAA;AAAjB,K,EACA,mBAAA,CAMM,KANN,EAAA,UAAA,EAMM,CALJ,mBAAA,CAAuD,GAAvD,EAAA,UAAA,EAAuD,gBAAA,CAAA,IAAhC,IAAgC,CAA3B,IAAI,CAAC,IAAsB,EAAhB,OAAgB,EAAA,CAAvD,EAA8C;AAAA;AAA9C,KAKI,EAJJ,mBAAA,CAGI,GAHJ,EAAA,UAAA,EAGI,CAFF,mBAAA,CAAqD,MAArD,EAAqD,IAArD,EAAqD,gBAAA,CAAA,IAAxC,IAAwC,CAAnC,IAAI,CAAC,IAA8B,EAAxB,QAAwB,KAAhB,CAAgB,CAArD,EAAqC;AAAA;AAArC,KAEE,EADF,mBAAA,CAAoD,MAApD,EAAoD,IAApD,EAAoD,gBAAA,CAAA,IAAvC,IAAuC,CAAlC,IAAI,CAAC,IAA6B,EAAvB,WAAuB,EAAA,CAApD,EAAwC;AAAA;AAAxC,KACE,CAHJ,CAII,CANN,C,EAOA,mBAAA,CAMM,KANN,EAAA,UAAA,EAMM,CALJ,mBAAA,CAGO,KAHP,EAGO;AAFL,MAAA,KAAK,EAAC,KAED;AADJ,MAAA,KAAK,EAAA,eAAA,CAAA;AAAA,QAAA,eAAA,gBAA4B,IAAA,CAAA,OAA5B,SAAsC,IAAI,CAAC,KAA3C;AAAA,OAAA;AACD,KAHP,E,IAAA,E;;AAAA,KAKI,EADJ,mBAAA,CAAqC,KAArC,EAAA,UAAA,EAAqC,gBAAA,CAAjB,IAAI,CAAC,GAAY,CAArC,EAA4B;AAAA;AAA5B,KACI,CANN,C,EAOA,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,mBAAA,CAAwB,GAAxB,EAAwB,IAAxB,EAAG,SAAI,gBAAA,CAAG,IAAI,CAAC,EAAR,CAAP,EAAiB;AAAA;AAAjB,KAEI,EADJ,YAAA,CAA4D,sBAA5D,EAA4D;AAA9C,MAAA,EAAE,qBAAc,IAAI,CAAC,GAAnB;AAA4C,KAA5D,E;wBAA0C;AAAA,eAAI,C,UAAA,CAAJ;AAAA,O;;;;KAA1C,E;;AAAA,M,MAAA,CACI,CAHN,C,CAhBF,C;GAoBU,CApBV,E;;AAAA,GAqBI,EAtBN,C,iCAAqC,QAAA,CAAA,I","sourcesContent":["\n<template>\n  <div id=\"Article\" v-infinite-scroll=\"load\">\n    <article class=\"fadeIn\" v-for=\"item in articleData\" :key=\"item._id\">\n      <h2>{{ item.title }}</h2>\n      <div class=\"time\">\n        <p class=\"date\">{{ new Date(item.date).getDate() }}</p>\n        <p class=\"m-y\">\n          <span>{{ new Date(item.date).getMonth() + 1 }}</span>\n          <span>{{ new Date(item.date).getFullYear() }}</span>\n        </p>\n      </div>\n      <div class=\"show\">\n        <div\n          class=\"img\"\n          :style=\"{ backgroundImage: `url(${baseURL}${item.cover})` }\"\n        ></div>\n        <div class=\"des\">{{ item.des }}</div>\n      </div>\n      <div class=\"r-b\">\n        <p>浏览量：{{ item.pv }}</p>\n        <router-link :to=\"`/article/${item._id}`\">阅读全文</router-link>\n      </div>\n    </article>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Article\",\n  data() {\n    return {\n      articleData: [],\n      skip: 0,\n      AllArticleData: [],\n    };\n  },\n  methods: {\n    async getArticles() {\n      let { data } = await this.$axios({\n        method: \"get\",\n        url: \"/get/article\",\n        params: { skip: this.skip },\n      });\n      console.log(data);\n      if (data.code) {\n        return this.$message.error(data.msg);\n      }\n      this.AllArticleData = data.data;\n      console.log(this.AllArticleData);\n      this.articleData.push(...this.AllArticleData.slice(this.skip, this.skip + 5));\n    },\n    load() {\n      if (!this.AllArticleData.length)return\n      console.log(666);\n        this.skip += 5;\n        this.articleData.push(...this.AllArticleData.slice(this.skip, this.skip + 5));\n      \n    },\n  },\n  created() {\n    this.getArticles();\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n#Article {\n  article {\n    box-sizing: border-box;\n    position: relative;\n    width: 100%;\n    margin-bottom: 15px;\n    box-shadow: 0 0 4px #ddd;\n    padding: 25px 20px;\n    background-color: #fff;\n    font-family: Quicksand, \"微软雅黑\";\n    h2 {\n      margin-bottom: 15px;\n      font-size: 20px;\n      letter-spacing: 3px;\n      line-height: 24px;\n      border-left: 5px solid #73b899;\n      text-indent: 10px;\n    }\n    .time {\n      position: absolute;\n      top: 0;\n      right: 10px;\n      width: 60px;\n      .date {\n        font-weight: bolder;\n        font-size: 40px;\n        text-align: center;\n        color: #6bc30d;\n      }\n      .m-y {\n        display: flex;\n        justify-content: space-between;\n      }\n    }\n    .show {\n      display: flex;\n      width: 100%;\n      height: 250px;\n      .img {\n        box-sizing: border-box;\n        width: 50%;\n        padding-right: 1%;\n        background-size: cover;\n        background-position: 50%;\n      }\n      .des {\n        box-sizing: border-box;\n        width: 50%;\n        padding-left: 1%;\n      }\n    }\n    .r-b {\n      position: absolute;\n      right: 20px;\n      bottom: 20px;\n      p {\n        position: relative;\n        top: -10px;\n        color: #aaa;\n        font-size: 12px;\n      }\n      a {\n        padding: 10px 20px;\n        background-color: #409eff;\n        color: #fff;\n        transition: opacity 0.3s;\n        &:hover {\n          opacity: 0.8;\n        }\n      }\n    }\n    &.fadeIn{\n      animation: fadeIn 1s 1;\n      animation-fill-mode: backwards;\n    }\n    @keyframes fadeIn {\n      from{\n        opacity: 0;\n        transform: scale(.5);\n      }\n      to{\n        opacity: 1;\n        transform: scale(1);\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}