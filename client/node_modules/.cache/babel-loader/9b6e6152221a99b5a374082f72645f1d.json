AP,EAAY;AACR,eAAO,KAAK,QAAL,CAAc,KAAd,CAAoB,YAApB,CAAP;AACJ,OAP4B,CAQ3B;;;AACA,WAAK,QAAL,GAAgB,GAAG,CAAC,eAAJ,CAAoB,IAAI,CAAC,GAAzB,CAAhB,CAT2B,CAU3B;;AACA,WAAK,YAAL,CAAkB,GAAG,CAAC,IAAtB;AACH,KAdI;AAeL,IAAA,kBAfK,8BAec,IAfd,EAeoB;AACvB,MAAA,OAAO,CAAC,GAAR,CAAY,IAAZ,EADuB,CAEvB;;AACA,UAAM,KAAI,GAAI,sBAAsB,IAAtB,CAA2B,IAAI,CAAC,IAAhC,CAAd;AACA,UAAM,MAAK,GAAI,IAAI,CAAC,IAAL,GAAY,IAAZ,GAAmB,IAAnB,GAA0B,GAAzC;;AAEA,UAAI,CAAC,KAAL,EAAY;AACV,aAAK,QAAL,CAAc,KAAd,CAAoB,qBAApB;AACF;;AACA,UAAI,CAAC,MAAL,EAAa;AACX,aAAK,QAAL,CAAc,KAAd,CAAoB,qBAApB;AACF;;AACA,aAAO,KAAI,IAAK,MAAhB;AACD;AA5BI;AANM,CAAf","sourcesContent":["<template>\r\n  <div id=\"UpdatePhoto\">\r\n      <el-alert\r\n      title=\"上传头像\"\r\n      description=\"点击下方 + 号 即可更新头像\"\r\n      type=\"warning\"\r\n      :closable='false'\r\n      show-icon>    \r\n      </el-alert>\r\n    <el-upload\r\n      class=\"avatar-uploader\"\r\n      :with-credentials=\"true\"\r\n      :action=\"baseURL+'/personal/photo'\"\r\n      :show-file-list=\"false\"\r\n\r\n      :on-success=\"handleAvatarSuccess\"\r\n      :before-upload=\"beforeAvatarUpload\"\r\n    >\r\n      <img v-if=\"imageUrl\" :src=\"imageUrl\" class=\"avatar\" />\r\n      <i v-else class=\"el-icon-plus avatar-uploader-icon \"></i>\r\n    </el-upload>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapMutations} from 'vuex'\r\nexport default {\r\n  data() {\r\n    return {\r\n      imageUrl: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapMutations(['loginSuccess']),\r\n    handleAvatarSuccess(res, file) {\r\n        /* 上传头像ui组件默认发送请求不携带cookie 所以请求发送到后台之后\r\n            由于请求并未携带cookie 所以后台会认为其并未登录 所以会被鉴权\r\n            路由拦截 发送给前端的信息为用户未登录\r\n        */\r\n       if(res.code){\r\n           return this.$message.error(\"上传失败，请稍后再试\")\r\n       }\r\n        //显示缩略图\r\n        this.imageUrl = URL.createObjectURL(file.raw)\r\n        //更新vuex\r\n        this.loginSuccess(res.data)\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      console.log(file);\r\n      //console.log(\"this.imageUrl\",this.imageUrl);\r\n      const isJPG = /^image\\/(jpeg|png)$/.test(file.type);\r\n      const isLt2M = file.size / 1024 / 1024 < 0.5;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error(\"上传头像图片只能为JPG|PNG格式!\");\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 500KB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n#UpdatePhoto {\r\n  /deep/.el-upload {\r\n    border: 1px dashed #d9d9d9;\r\n    border-radius: 6px;\r\n    cursor: pointer;\r\n    position: relative;\r\n    overflow: hidden;\r\n    margin-top: 10px;\r\n    .avatar{\r\n        display: block;\r\n        max-width: 178px;\r\n        max-height: 178px;\r\n    }\r\n    .avatar-uploader-icon{\r\n      font-size: 28px;\r\n      color: #8c939d;\r\n      width: 178px;\r\n      height: 178px;\r\n      line-height: 178px;\r\n      text-align: center;\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}