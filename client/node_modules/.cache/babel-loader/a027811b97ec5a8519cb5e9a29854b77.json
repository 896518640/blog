n    LinkManager,\n    ArticleAdd,\n    ArticleManger,\n    ContactMsg,\n  },\n  computed: {\n    ...mapState([\"userInfo\"]),\n  },\n  watch: {\n    userInfo() {\n      this.check();\n    },\n  },\n  methods: {\n    async check() {\n      //像后端发送请求 验证管理员权限\n      let { data } = await this.$axios({\n        method: \"POST\",\n        url: \"/adminServer/check\",\n      });\n      console.log(data);\n      if (data.code) {\n        this.$router.replace(\"/\");\n        this.$message.error(\"请以管理员账号登录\");\n        /*  this.$router.go(-1); */\n      }\n    },\n  },\n  created() {\n    this.check();\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n#Admin {\n  article {\n    box-sizing: border-box;\n    width: 100%;\n    margin-bottom: 15px;\n    box-shadow: 0 0 4px #ddd;\n    padding: 25px 20px;\n    background-color: #fff;\n    min-height: 721px;\n    font-family: Quicksand, Microsoft YaHei, sans-serif;\n    h2 {\n      margin-bottom: 15px;\n      font-size: 20px;\n      letter-spacing: 3px;\n      line-height: 24px;\n      border-left: 5px solid #73b899;\n      text-indent: 10px;\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}