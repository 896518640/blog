el-input\n        type=\"textarea\"\n        :row=\"2\"\n        placeholder=\"ËØ∑ËæìÂÖ•ÂÜÖÂÆπ\"\n        v-model=\"textarea\"\n        maxlength=\"100\"\n        show-word-limit\n      ></el-input>\n        <div class=\"btn\">\n          <div class=\"left\">\n            <i class=\"iconfont icon-biaoqing\" @click.stop=\"showEmoji\"></i>\n            <div class=\"emoji\" v-if=\"ifShowEmoji\">\n              <ul>\n                <li v-for=\"item in emoji\" \n                :key=\"item\"\n                @click.stop=\"clickEmoji(item)\"\n                >{{item}}</li>\n              </ul>\n            </div>\n          </div>\n          <el-button \n            type=\"primary\"\n            size=\"mini\"\n            @click=\"submit\"\n            >ÂèëË°®</el-button>\n        </div>\n    </article>\n    <article class=\"a2\">\n      <ul>\n        <li v-for=\"item in msgList\" :key=\"item._id\">\n          <div class=\"parent\">\n            <div \n            class=\"left\"\n            :style=\"{backgroundImage:`url(${baseURL}${item.author.photo})`}\"\n            ></div>\n            <div class=\"right\">\n              <div class=\"name-time \">\n                <span>{{item.author.user}}</span>\n                <span>{{formatDate(item.date)}}</span>\n              </div>\n              <div class=\"message\">\n                {{item.text }}\n              </div>\n              <div class=\"like-reply\">\n                <i class=\"like iconfont icon-xinaixin \"\n                :class=\"{isLike:userInfo._id && item.likes.includes(userInfo._id)}\"\n                @click=\"parentLikeClick(item._id)\"></i>\n                <span>{{item.likes.length}}</span>\n                <i class=\"reply iconfont icon-huifu\"\n                @click=\"parentReplyClick(item)\"></i>\n              </div>\n            </div>\n          </div>\n          <div class=\"child\" v-for=\"(childItem,cindex) in item.children\" :key=\"childItem._id\">\n            <div \n            class=\"left\"\n            :style=\"{backgroundImage:`url(${baseURL}${childItem.author.photo})`}\"\n            ></div>\n            <div class=\"right\">\n              <div class=\"name-time \">\n                <span>{{childItem.author.user}}</span>\n                <span>2{{formatDate(childItem.date)}}</span>\n              </div>\n              <div class=\"message\">\n                <span>@{{childItem.replyUser.user}}</span>{{childItem.text}}\n              </div>\n              <div class=\"like-reply\">\n                <i class=\"like iconfont icon-xinaixin\"\n                :class=\"{isLike:userInfo._id&&childItem.likes.includes(userInfo._id)}\"\n                @click=\"childLikeClick(item._id,childItem._id,cindex)\"\n                ></i>\n                <span>{{childItem.likes.length}}</span>\n                <i class=\"reply iconfont icon-huifu\"\n                  @click=\"childReplyClick(item,childItem)\"\n                ></i>\n              </div>\n            </div>\n          </div>\n          <div class=\"replyInput \"\n          :class=\"{show:item.ifShowReply}\"\n          >\n            <el-input size=\"mini\" \n            :placeholder=\"`@${item.replyUser.user}`\"\n            v-model=\"item.replyTxt\"\n            ></el-input>\n            <el-button \n              type=\"primary\"\n               size=\"mini\"\n               @click=\"replySubmit(item)\">ÁïôË®Ä</el-button>\n          </div>\n        </li>\n      </ul>\n    </article>\n  </div>\n</template>\n\n<script>\nimport {mapState} from 'vuex'\nexport default {\n    name:\"Message\",\n    data(){\n      return{\n        //ÁïôË®ÄÂÜÖÂÆπ\n        textarea:\"\",\n        //Áî®‰∫éÊòæÁ§∫/ÈöêËóè emojiÈÄâÊã©Ê°Ü\n        ifShowEmoji:false,\n        //emojiÊï∞ÁªÑ\n        emoji:[\n          \"üòÄ\",\"üòÅ\",\"üòÇ\",\"ü§£\",\"üòÉ\",\"üòÑ\",\"üòÖ\",\"üòÜ\",\"üòâ\",\"üòä\",\"üòã\",\n          \"üòé\",\"üòç\",\"üòò\",\"ü•∞\",\"üòó\",\"üòô\",\"ü•≤\",\"üòö\",\"üôÇ\",\"ü§ó\",\"üéà\",\n          \"ü§©\",\"ü§î\",\"ü§®\",\"üòê\",\"üòë\",\"üò∂\",\"üôÑ\",\"üòè\",\"üò£\",\"üò•\",\"üòÆ\",\n          \"ü§ê\",\"üòØ\",\"üò™\",\"üò´\",\"ü•±\",\"üò¥\",\"üòå\",\"üòõ\",\"üòú\",\"üòù\",\"ü§§\",\n          \"üòí\",\"üòì\",\"üòî\",\"üòï\",\"üôÉ\",\"ü§ë\",\"üòû\",\"üòü\",\"üò≤\",\"‚òπÔ∏è\",\"üò§\",\n          \"üò¢\",\"üò≠\",\"üôÅ\",\"üò¶\",\"üòñ\",\"üò∞\",\"üò¨\",\"ü§Ø\",\"üò©\",\"üò®\",\"üòß\",\n          \"üò±\",\"ü•µ\",\"ü•∂\",\"üò≥\",\"ü§™\",\"üòµ\",\"ü§í\",\"üò∑\",\"ü§¨\",\"üò°\",\"üò†\",\n          \"ü•¥\",\"ü§ï\",\"ü§¢\",\"ü§Æ\",\"ü§ß\",\"üòá\",\"ü•≥\",\"ü§´\",\"ü§•\",\"ü§°\",\"ü§†\",\n          \"ü•∫\",\"ü•∏\",\"ü§≠\",\"üßê\",\"ü§ì\",\"üòà\",\"üëø\",\"üëπ\",\"üë∫\",\"üíÄ\",\"‚ò†Ô∏è\",\n          \"üëª\",\"üëΩ\",\"üí™\",\"ü¶µ\",\"ü¶∂\",\"üëÇ\",\"ü¶ª\",\"üëÉ\",\"ü§è\",\"üëà\",\"üëå\",\n          \"üëâ\",\"‚òùÔ∏è\",\"üëÜ\",\"üëá\",\"‚úåÔ∏è\",\"ü§û\",\"üññ\",\"ü§ò\",\"ü§ô\",\"üñêÔ∏è\",\"‚úã\",\n          \"ü§å\",\"üëç\",\"üëé\",\"‚úä\",\"üëä\",\"‚úçÔ∏è\",\"ü§ù\",\"üíÖ\",\"üëè\",\"ü§õ\",\"ü§ú\",\n          \"ü§ö\",\"üëã\",\"ü§ü\",\"üôè\",\"ü§≤\",\"üôå\",\"üëê\",\"üíù\",\"üíò\",\"üíñ\",\"üíó\",\n          \"üíì\",\"üíû\",\"üíï\",\"‚ù£Ô∏è\",\"üíî\",\"‚ù§Ô∏è\",\"üß°\",\"üíü\",\"üíå\",\"üí¢\",\"üîû\"\n        ],\n        //ÊâÄÊúâÁïôË®ÄÂàóË°®\n        msgList:[\n\n        ]\n      }\n    },\n    computed:{\n      ...mapState(['userInfo'])\n    },\n    methods:{\n      //ÊòæÁ§∫Ë°®ÊÉÖÊ°Ü\n      showEmoji(){\n        this.ifShowEmoji = !this.ifShowEmoji\n      },\n      //Ë°®ÊÉÖÂõæÊ†áÁÇπÂáª\n      clickEmoji(item){\n        console.log(this.textarea.length);\n        if(this.textarea.length>98)return\n        \n        this.textarea+=item\n      },\n      //Êèê‰∫§ÁïôË®Ä ÂÜôÂú®methodsÁöÑÂáΩÊï∞ÂèØ‰ª•Áõ¥Êé•Âä†async\n      async submit(){\n        //Ê≤°ÊúâÁôªÈôÜÁöÑËØù ÁªôÊèêÁ§∫\n        //console.log(this.userInfo);\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        //ÁïôË®ÄÂÜÖÂÆπ‰∏∫Á©∫\n        let val = this.textarea.trim()\n        if(!val){\n          return this.$message.error(\"ËØ∑ËæìÂÖ•ÂÜÖÂÆπ\")\n        } \n\n        //ÂèëÈÄÅÂà∞ÂêéÁ´Ø\n        let {data} = await this.$axios({\n          method:\"POST\",\n          url:\"/msg/submit\",\n          data:{\n            val\n          }\n        })\n        //ÁïôË®ÄÂ§±Ë¥•\n        if(data.code){\n          return $message.error(data.msg)//ÊèêÁ§∫\n        }\n        //ÁïôË®ÄÊàêÂäü\n        this.$message.success(\"ÁïôË®ÄÊàêÂäüÂï¶\")//ÊèêÁ§∫\n        this.textarea = \"\"//ÂÜÖÂÆπÊ∏ÖÁ©∫\n        this.getMsg()\n      },\n      //ËØ∑Ê±ÇÊâÄÊúâÁïôË®Ä\n      async getMsg(){\n          let {data} = await this.$axios({\n            method:\"GET\",\n            url:\"/get/msg\"\n          })\n          let d = data.data\n          d.forEach(item=>{\n            item.ifShowReply = false//Áî®‰∫éÊéßÂà∂ÂõûÂ§çÊ°ÜÊòæÁ§∫/ÈöêËóè\n            item.replyTxt=\"\"// ÂõûÂ§çÊ°ÜÁöÑÂÜÖÂÆπÁªëÂÆö\n            item.replyUser=item.author//ÊèêÁ§∫Ê°ÜÊòæÁ§∫ÊñáÊú¨ÂÜÖÂÆπ\n          })\n          console.log(d);\n          this.msgList = d\n      },\n      //ËΩ¨Êç¢Êó•ÊúüÊòæÁ§∫\n      formatDate(date){\n        let d = new Date(date),\n        YY = d.getFullYear(),\n        MM = d.getMonth()+1,\n        DD = d.getDate(),\n        hh = d.getHours(),\n        mm = d.getMinutes(),\n        ss = d.getSeconds()\n        MM = (MM < 10? \"0\":\"\") +MM\n        DD = (DD < 10? \"0\":\"\") +DD\n        hh = (hh < 10? \"0\":\"\") +hh\n        mm = (mm < 10? \"0\":\"\") +mm\n        ss = (ss < 10? \"0\":\"\") +ss\n        return `${YY}-${MM}-${DD} ${hh}:${mm}:${ss}`\n      },\n      //Áà∂Á∫ßÁÇπËµûÊåâÈíÆ\n      async parentLikeClick(id){\n        //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        let {data} = await this.$axios({\n            method:\"POST\",\n            url:\"/msg/like/parent\",\n            data:{\n              id//ËØÑËÆ∫id\n            }\n        })\n        console.log(data);\n        //Êìç‰ΩúÂ§±Ë¥•\n        if(data.code){\n          return this.$message.error(data.msg)\n        }\n        //Êìç‰ΩúÊàêÂäü\n        this.getMsg()\n      },\n      //ÂõûÂ§çÊåâÈíÆÁöÑÁÇπÂáª\n      parentReplyClick(item){\n        //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        //ÈÄöËøáÂà§Êñ≠@ÁöÑÂêçÂ≠ó‰∏ÄÊ†∑‰∏ç‰∏ÄÊ†∑ Êù•ÂÜ≥ÂÆöÊòØÂê¶Â±ïÂºÄ„ÄÅ\n        if(item.author._id !==item.replyUser._id){\n          item.ifShowReply = true\n        }else{\n          //ÊéßÂà∂ÊòæÁ§∫ÈöêËóè\n        item.ifShowReply =!item.ifShowReply\n        }\n        //ÊîπÂèò@ÂêéÈù¢ÁöÑÂêçÂ≠ó\n        item.replyUser = item.author\n      },\n      //ÂõûÂ§çÁöÑÊèê‰∫§\n      async replySubmit(item){\n        //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        let text = item.replyTxt.trim()\n        //È™åËØÅÊï∞ÊçÆÊ†ºÂºè\n        if(!text){\n          return this.$message.error(\"ËØ∑ËæìÂÖ•ÂõûÂ§çÂÜÖÂÆπ\")\n        }\n        if(text.length>100){\n          return this.$message.error(\"ÂõûÂ§çÂ≠óÊï∞‰∏çËÉΩË∂ÖËøá100\")\n        }\n\n        //ÂèëÈÄÅÊï∞ÊçÆÂà∞ÂêéÁ´Ø\n        let {data} = await this.$axios({\n          method:\"POST\",\n          url:\"/msg/reply/submit\",\n          data:{\n            id:item._id,\n            text,\n            replyUser:item.replyUser._id\n          }\n        })\n        if(data.code){\n          return this.$message.error(data.msg)\n        }\n        this.$message.success(\"ÂõûÂ§çÊàêÂäü\")\n        this.getMsg()\n      },\n      //Â≠êÂõûÂ§çÁÇπËµû\n      async childLikeClick(pid,cid,cindex){\n        //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        //ÂèëÈÄÅËØ∑Ê±Ç\n        let {data} = await this.$axios({\n          method:\"POST\",\n          url:\"/msg/like/child\",\n          data:{pid,cid,cindex}\n        })\n        //console.log(data);\n\n        if(data.code){\n          return this.$message.error(data.msg)\n        }\n        this.getMsg()\n      },\n      //Â≠êÂõûÂ§çÊåâÈíÆÁÇπÂáª\n      childReplyClick(item,cItem){\n        //Âà§Êñ≠Áî®Êà∑ÊòØÂê¶ÁôªÂΩï\n        if(!this.userInfo.user){\n          return this.$message.error(\"ËØ∑ÂÖàÁôªÂΩï\")\n        }\n        //ÈÄöËøáÂà§Êñ≠@ÁöÑÂêçÂ≠ó‰∏ÄÊ†∑‰∏ç‰∏ÄÊ†∑ Êù•ÂÜ≥ÂÆöÊòØÂê¶Â±ïÂºÄ„ÄÅ\n        if(cItem.author._id !==item.replyUser._id){\n          item.ifShowReply = true\n        }else{\n          item.ifShowReply = !item.ifShowReply\n        } \n        //ÊîπÂèòreplyUser\n        item.replyUser = cItem.author\n        \n      }\n  },\n    mounted(){\n      //ÁÇπÂáªdocument emojiÊ∂àÂ§±\n      document.addEventListener(\"click\",()=>{\n        this.ifShowEmoji = false\n      })\n    },\n    created(){\n      //ËØ∑Ê±ÇÁïôË®ÄÊï∞ÊçÆ\n      this.getMsg()\n      console.log(111);\n    }\n}\n</script>\n\n<style lang=\"less\" scoped>\n#Message{\n  article {\n        box-sizing: border-box;\n        width: 100%;\n        margin-bottom: 15px;\n        box-shadow: 0 0 4px #ddd;\n        padding: 25px 20px ;\n        background-color: #fff;\n        font-family: Quicksand,Microsoft YaHei,sans-serif;\n        h2 {\n            margin-bottom: 15px;\n            font-size: 20px;\n            letter-spacing: 3px;\n            line-height: 24px;\n            border-left: 5px solid #73b899;\n            text-indent: 10px;\n        }\n        p {\n            text-indent: 2em;\n            color: #444;\n            font-size: 14px;\n            letter-spacing: 4px;\n            line-height: 30px;\n        } \n    }\n  article.a1{\n    .el-textarea {\n          margin-top: 15px;\n    }\n  /* Ê†∑ÂºèÁ©øÈÄè */\n  /deep/ textarea{\n          font-family: \"Quicksand\",\"ÂæÆËΩØÈõÖÈªë\";\n          letter-spacing: 5px;\n          height: 70px;\n          font-size: 12px;\n    }\n    .btn{\n        display: flex;\n        margin-top: 10px;\n        justify-content: space-between;\n        .left{\n          position: relative;\n        }\n        i{\n          font-size: 20px;\n          cursor:pointer;\n      }\n      .emoji{\n        position: absolute;\n        top:30px;\n        background-color: #fff;\n        padding: 8px;\n        z-index: 10;\n        box-shadow: 0 0 3px #aaa;\n        &::before{\n          content: \"\";\n          position: absolute;\n          z-index: 3;\n          top: -10px;\n          left: 5px;\n          width: 0;\n          height: 0;\n          border: 5px solid transparent;\n          border-bottom-color: #ccc;\n        }\n        ul{\n          display: flex;\n          flex-wrap: wrap;\n          width: 275px;\n          border-left: 1px solid #e5e5e5;\n          border-bottom: 1px solid #e5e5e5;\n          li{\n            box-sizing: border-box;\n            float: left;\n            width: 25px;\n            height: 25px;\n            border-right: 1px solid #e5e5e5;\n            border-top: 1px solid #e5e5e5;\n            font-size: 18px;\n            line-height: 25px;\n            cursor: pointer;\n          }\n        }\n      }\n    }\n  }\n  article.a2{\n    ul li{\n      padding-bottom: 5px;\n      margin-bottom: 10px;\n      border-bottom: 1px dashed #ddd;\n      font-size: 12px;\n    .child{\n      margin-left: 55px;\n      margin-top: 3px;\n      }\n    .parent,.child{\n      display: flex;\n    .left{\n      overflow: hidden;\n      width: 40px;\n      height: 40px;\n      margin-right: 15px;\n      border-radius: 50%;\n      background: none top center/cover;\n    }\n    .right{\n      flex: 1;\n      .name-time{\n        font-size: 12px;\n        margin-bottom: 3px;\n        span:nth-child(1){\n          color:#73b899\n        }\n        span:nth-child(2){\n          color:#aaa;\n          margin-left: 10px;\n        }\n      }\n      .message{\n        color: #666;\n        letter-spacing: 2px;\n        margin-bottom: 3px;\n        word-break: break-word;\n        span{\n          color: #409eff;\n          margin-right: 5px;\n        }\n      }\n      .like-reply{\n         i{\n           cursor: pointer;\n           font-size: 14px;\n           color: #aaa;\n         }\n         i.isLike{\n           color:#f30606;\n         }\n         span{\n           display: inline-block;\n           width: 43px;\n           font-size: 12px;\n           color: #aaa;\n         }\n      }\n    }\n      }\n    .replyInput{\n      overflow: hidden;\n      transition: height .3s;\n      height: 0px;\n      width: 100%;\n      padding-left: 55px;\n      box-sizing: border-box;\n      margin-top: 10px;\n      &.show{\n        height: 57px;\n      }\n      .el-button{\n        padding: 0px 9px!important;\n        float: right;\n      }\n    }\n    }\n  }\n}\n   \n</style>"],"sourceRoot":""}]}