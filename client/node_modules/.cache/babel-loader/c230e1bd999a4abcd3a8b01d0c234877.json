A,MAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,gBAAtB,EAjBqB,CAkBrB;;;AACA,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,WAAnB,GAnBqB,CAoBrB;;;AACA,kBAAA,MAAI,CAAC,UAAL,GAAkB,OAAlB;AArBqB;AAAA;;AAAA;AAAA,mDAuBd,KAvBc;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAA7B;;AAAA;AAAA;AAAA;AAAA;AA0BH,KArCI;AAsCL;AACA,IAAA,WAvCK,yBAuCQ;AAAA;;AACT,WAAK,KAAL,CAAW,WAAX,EAAwB,QAAxB;AAAA,4EAAiC,kBAAM,CAAN;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,uBACzB,CADyB;AAAA;AAAA;AAAA;;AAAA;AAAA,yBAGL,MAAI,CAAC,MAAL,CAAY;AAC3B,oBAAA,MAAM,EAAC,MADoB;AAE3B,oBAAA,GAAG,EAAC,QAFuB;AAG3B,oBAAA,IAAI,EAAC,MAAI,CAAC;AAHiB,mBAAZ,CAHK;;AAAA;AAAA;AAGnB,kBAAA,IAHmB,wBAGnB,IAHmB;AAQxB,kBAAA,OAAO,CAAC,GAAR,CAAY,IAAZ,EARwB,CASxB;;AATwB,uBAUrB,IAAI,CAAC,IAVgB;AAAA;AAAA;AAAA;;AAWtB;AACA,kBAAA,MAAI,CAAC,QAAL,CAAc,KAAd,CAAoB,IAAI,CAAC,GAAzB;;AAZsB;;AAAA;AAexB;AACA,kBAAA,MAAI,CAAC,QAAL,CAAc,OAAd,CAAsB,MAAtB,EAhBwB,CAgBM;;;AAC9B,kBAAA,MAAI,CAAC,KAAL,CAAW,SAAX,CAAqB,WAArB,GAjBwB,CAiBW;;;AACnC,kBAAA,MAAI,CAAC,MAAL,GAAc,KAAd,CAlBwB,CAkBJ;AACpB;;AACA,kBAAA,MAAI,CAAC,YAAL,CAAkB,IAAI,CAAC,IAAvB;;AApBwB;AAAA;;AAAA;AAAA,oDAsBjB,KAtBiB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAjC;;AAAA;AAAA;AAAA;AAAA;AAyBH;AAjEI,KAmEF,YAAY,CAAC,CAAC,cAAD,CAAD,CAnEV;AA/DM,CAAf","sourcesContent":["<template>\r\n  <el-button type=\"success\" size=\"mini\" @click=\"showloginBox\"\r\n    >登录/注册</el-button\r\n  >\r\n  <div class=\"loginBox\" @click=\"closeLoginBox\" v-if=\"ifShow\">\r\n      <div class=\"main\" @click.stop >\r\n           <el-tabs v-model=\"activeName\" >\r\n                <el-tab-pane label=\"立即登录\" name=\"first\">\r\n                    <el-form\r\n                      :model=\"loginForm\"\r\n                      status-icon\r\n                      ref=\"loginForm\"\r\n                      :rules=\"rules\"\r\n                      label-width=\"80px\"\r\n                      @keydown.enter=\"loginSubmit\"\r\n                    >   \r\n                        <el-form-item label=\"用户名\" prop=\"user\">\r\n                            <el-input v-model=\"loginForm.user\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"密码\" prop=\"pass\">\r\n                            <el-input type=\"password\" v-model=\"loginForm.pass\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item >\r\n                            <el-button type=\"primary\" @click=\"loginSubmit()\">登录</el-button>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </el-tab-pane>\r\n                <el-tab-pane label=\"注册账号\" name=\"second\">\r\n                    <el-form\r\n                      :model=\"regForm\"\r\n                      status-icon\r\n                      ref=\"regForm\"\r\n                      :rules=\"rules\"\r\n                      label-width=\"80px\"\r\n                     @keydown.enter=\"regSubmit\"\r\n                    >   \r\n                        <el-form-item label=\"用户名\" prop=\"user\">\r\n                            <el-input v-model=\"regForm.user\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"密码\" prop=\"pass\">\r\n                            <el-input type=\"password\" v-model=\"regForm.pass\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"确认密码\" prop=\"pass2\">\r\n                            <el-input type=\"password\" v-model=\"regForm.pass2\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item >\r\n                            <el-button type=\"primary\" @click=\"regSubmit('regForm')\">立即注册</el-button>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </el-tab-pane>\r\n            </el-tabs>\r\n      </div>\r\n   \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapMutations} from 'vuex'\r\nexport default {\r\n  data() {\r\n    return {\r\n      ifShow: false,\r\n      activeName:'first',\r\n      //登陆表单数据\r\n      loginForm:{\r\n          user:\"\",\r\n          pass:\"\"\r\n      },\r\n      //注册表单数据\r\n      regForm:{\r\n          user:\"\",\r\n          pass:\"\",\r\n          pass2:\"\"\r\n      },\r\n      rules:{\r\n          user:[\r\n              {required:true,message:\"请输入用户名\"},\r\n              {\r\n                  validator(rule,value,cb){\r\n                      //长度检验\r\n                      if(value.length<2 || value.length>8){\r\n                          cb(new Error(\"请输入2~8位的用户名\"))\r\n                      }\r\n                      //规则验证\r\n                      if(!/^[\\w\\u4e00-\\u9fa5\\u0800-\\u4e00\\uac00-\\ud7ff]+$/.test(value)){\r\n                          cb(new Error(\"只允许字母、数字、_、中/日/韩文\"))\r\n                      }\r\n                      cb()\r\n                  }\r\n              }\r\n          ],\r\n          pass:[\r\n              {required:true,message:\"请输入密码\"},\r\n              {\r\n                  validator(rule,value,cb){\r\n                      //长度验证\r\n                      if(value.length<6||value.length>18){\r\n                          cb(new Error(\"请输入6~18位的密码\"))\r\n                      }\r\n                      //规则验证\r\n                      if(!/^[\\w!\\[\\]{}\\/\\\\:\"|?>·`<@#$%^&*\\(\\)_+=~\\-]+$/.test(value)){\r\n                           cb(new Error(\"请输入合法的英文字符\"))\r\n                      }\r\n                      cb()\r\n                  }\r\n              }\r\n          ],\r\n          pass2:[\r\n              {required:true,message:\"请确认密码\"},\r\n              {\r\n                  validator:(rule,value,cb)=>{\r\n                     if(this.regForm.pass !==value){\r\n                         cb(new Error(\"两次输入密码不一致\"))\r\n                     }\r\n                     cb()\r\n                  }\r\n              }\r\n          ]\r\n        }\r\n    }\r\n  },\r\n  methods: {\r\n    // 关闭 登录/注册 弹窗\r\n    closeLoginBox(){\r\n        this.ifShow = false\r\n    },\r\n    // 打开 登录/注册 弹窗\r\n    showloginBox() {\r\n      this.ifShow = true;\r\n    },\r\n    // 注册提交\r\n    regSubmit(regForm){\r\n        this.$refs[regForm].validate(async v=>{\r\n            console.log(111);\r\n            if(v){\r\n               let {data} = await this.$axios({\r\n                    method:\"POST\",\r\n                    url:\"/reg\",\r\n                    data:{\r\n                        user:this.regForm.user,\r\n                        pass:this.regForm.pass\r\n                    }\r\n                })\r\n                //注册失败\r\n                if(data.code){\r\n                    this.$message.error(data.msg)\r\n                    return\r\n                }\r\n                //注册成功\r\n                this.$message.success(\"注册成功，即将为您切换到登录\")\r\n                //清空表单数据\r\n                this.$refs.regForm.resetFields()\r\n                //切换到登陆页面\r\n                this.activeName = \"first\"\r\n            }else{\r\n                return false\r\n            }\r\n        })\r\n    },\r\n    // 登陆提交\r\n    loginSubmit(){\r\n        this.$refs['loginForm'].validate(async v=>{\r\n             if(v){\r\n                 //console.log(666);\r\n                 let {data} = await this.$axios({\r\n                     method:\"POST\",\r\n                     url:\"/login\",\r\n                     data:this.loginForm\r\n                 })\r\n                 console.log(data);\r\n                 //登陆失败\r\n                 if(data.code){\r\n                   // console.log(1111);\r\n                   this.$message.error(data.msg)\r\n                   return\r\n                 }\r\n                 //登陆成功 \r\n                 this.$message.success(\"登陆成功\") //成功提示\r\n                 this.$refs.loginForm.resetFields() // 清空表单数据\r\n                 this.ifShow = false // 关闭弹窗\r\n                 //存到vuex\r\n                 this.loginSuccess(data.data)\r\n             }else{\r\n                 return false\r\n             }\r\n        })\r\n    },\r\n    //vuex\r\n    ...mapMutations(['loginSuccess'])\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n.loginBox {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 1000;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  .main{\r\n      width: 300px;\r\n      background-color: #fff;\r\n      position: absolute;\r\n      top: 50%;\r\n      left: 50%;\r\n      padding: 10px 20px;\r\n      transform: translate(-50%,-50%);\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}